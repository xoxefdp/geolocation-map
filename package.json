{
  "name": "geolocation-map",
  "version": "1.2.2",
  "description": "Locate position using device geolocation api, leaflet interactive map and openstreet layer",
  "author": "Jos√© Diaz <contact@xisco.dev> (https://xisco.dev)",
  "license": "ISC",
  "keywords": [
    "geolocation",
    "openstreetmap",
    "leaflet",
    "vuejs"
  ],
  "private": true,
  "main": "dist/bundle.js",
  "scripts": {
    "serve": "webpack-dev-server --progress --display-error-details --config webpack.config.js",
    "serve:umd:dev": "BUILD_TYPE=umd NODE_ENV=development npm run serve",
    "serve:umd:prod": "BUILD_TYPE=umd NODE_ENV=production npm run serve",
    "serve:commonjs:dev": "BUILD_TYPE=commonjs NODE_ENV=development npm run serve",
    "serve:commonjs:prod": "BUILD_TYPE=commonjs NODE_ENV=production npm run serve",
    "serve:bundlers:dev": "BUILD_TYPE=bundlers NODE_ENV=development npm run serve",
    "serve:bundlers:prod": "BUILD_TYPE=bundlers NODE_ENV=production npm run serve",
    "serve:browsers:dev": "BUILD_TYPE=browsers NODE_ENV=development npm run serve",
    "serve:browsers:prod": "BUILD_TYPE=browsers NODE_ENV=production npm run serve",
    "prebuild": "npm run clean",
    "build": "webpack --config webpack.config.js",
    "build:umd:dev": "BUILD_TYPE=umd NODE_ENV=development npm run build",
    "build:umd:prod": "BUILD_TYPE=umd NODE_ENV=production npm run build",
    "build:commonjs:dev": "BUILD_TYPE=commonjs NODE_ENV=development npm run build",
    "build:commonjs:prod": "BUILD_TYPE=commonjs NODE_ENV=production npm run build",
    "build:bundlers:dev": "BUILD_TYPE=bundlers NODE_ENV=development npm run build",
    "build:bundlers:prod": "BUILD_TYPE=bundlers NODE_ENV=production npm run build",
    "build:browsers:dev": "BUILD_TYPE=browsers NODE_ENV=development npm run build",
    "build:browsers:prod": "BUILD_TYPE=browsers NODE_ENV=production npm run build",
    "build:serve": "npx http-server ./dist -p 9000",
    "clean": "rimraf dist",
    "postclean": "npm run test:cover",
    "test": "npm run lint && jest --verbose --runInBand",
    "test:watch": "npm run test -- --watch",
    "test:watch-all": "npm run test -- --watchAll",
    "test:cover": "npm run test -- --coverage",
    "lint": "eslint --fix \"src/**/*.js\""
  },
  "pre-commit": [
    "lint"
  ],
  "dependencies": {
    "leaflet": "^1.6.0",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2",
    "lodash.unset": "^4.5.2",
    "pubsub-js": "^1.9.3",
    "the-type-validator": "^1.1.0",
    "vue": "^2.6.10",
    "vue2-leaflet": "^2.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/preset-env": "^7.13.9",
    "@vue/test-utils": "^1.1.3",
    "babel-jest": "^26.6.3",
    "css-loader": "^3.2.0",
    "eslint": "^7.21.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.5",
    "eslint-plugin-jsdoc": "^32.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^4.1.0",
    "eslint-plugin-vue": "^6.1.2",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.12.0",
    "jest": "^26.6.3",
    "jest-runner-eslint": "^0.10.0",
    "pre-commit": "^1.2.2",
    "rimraf": "^3.0.0",
    "style-loader": "^1.0.0",
    "terser-webpack-plugin": "^4.2.3",
    "url-loader": "^2.2.0",
    "vue-jest": "^3.0.7",
    "vue-loader": "^15.7.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  }
}
