{
  "compilerOptions": {
    "target": "es6",
    "allowSyntheticDefaultImports": false,
    "baseUrl": "./",
    "paths": {
      "components/*": ["src/components"],
      "systems/*": ["src/systems"],
      "geolocation/*": ["src/systems/geolocation"],
      "permissions/*": ["src/systems/permissions"],
      "styles/*": ["src/styles"],
      "assets/*": ["src/assets"],
    }
  },
  "vueCompilerOptions": {
    // Volar preferentially supports Vue 3. Vue 3 and Vue 2 templates have some differences. You need to set the target option to support the Vue 2 templates.
    // "target": 2, // For Vue version <= 2.6.1
    "target": 2.7, // For Vue version >= 2.7.04
  },
  "exclude": ["node_modules", "dist"]
}
